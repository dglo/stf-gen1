<?xml version="1.0" encoding="UTF-8"?>
<stf:test xmlns:stf="http://glacier.lbl.gov/icecube/daq/stf"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://glacier.lbl.gov/icecube/daq/stf file:/Volumes/IceCube/WorkSpaces/junk/stf/private/stf-apps/stfDefn.xsd">
    <name>StfEg</name>
    <description>This defines a test that gives example of how to define, in
        XML, the different parts of a test.</description>
    <version major="1" minor="0"/>
    <fpgaDependencies>
        <COM_FIFO/>
	<DAQ/>
        <PULSERS/>
	<DISCRIMINATOR_RATE/>
	<LOCAL_COINC/>
        <FLASHER_BOARD/>
	<TRIGGER/>
	<LOCAL_CLOCK/>
	<SUPERNOVA/>
	<!-- <ALL/> would work too -->
    </fpgaDependencies>
    <inputParameter>
        <description>An input parameter which is a boolean.</description>
        <name>Para1</name>
        <boolean default="false"/>
    </inputParameter>
    <inputParameter>
         <description>An input parameter which is a string.</description>
       <name>Para2</name>
        <string default="bananas"/>
    </inputParameter>
    <inputParameter>
        <description>An input parameter which is an unsigned int.</description>
        <name>Para3</name>
        <unsignedInt default="0" maxValue="1000" minValue="0"/>
    </inputParameter>
    <inputParameter>
        <description>An input parameter which is an unsigned long.</description>
        <name>Para4</name>
        <unsignedLong default="0" maxValue="281474976710656" minValue="0"/>
    </inputParameter>
    <outputParameter>
        <description>A boolean output parameter.</description>
        <name>Para5</name>
        <boolean/>
    </outputParameter>
    <outputParameter>
        <description>A string output parameter.</description>
        <name>Para6</name>
        <string/>
    </outputParameter>
    <outputParameter>
        <description>An unsigned int output parameter.</description>
        <name>Para7</name>
        <unsignedInt/>
    </outputParameter>
    <outputParameter>
        <description>An unsigned long output parameter. </description>
        <name>Para8</name>
        <unsignedLong/>
    </outputParameter>
    <outputParameter>
        <description>An unsigned int array output parameter.</description>
	<name>Para9</name>
	<unsignedIntArray length="10" />
    </outputParameter>
</stf:test>

