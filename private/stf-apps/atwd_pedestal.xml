<?xml version="1.0" encoding="UTF-8"?>
<stf:test xmlns:stf="http://glacier.lbl.gov/icecube/daq/stf">
  <name>atwd_pedestal</name>
  <description>ATWD Pedestal</description>
  <version major="1" minor="0"/>
  <inputParameter>
    <name>atwd_sampling_speed_dac</name>
    <unsignedInt default="2000" maxValue="4095" minValue="0"/>
  </inputParameter>

  <inputParameter>
    <name>atwd_ramp_top_dac</name>
    <unsignedInt default="2400" maxValue="4095" minValue="0"/>
  </inputParameter>

  <inputParameter>
    <name>atwd_ramp_bias_dac</name>
    <unsignedInt default="350" maxValue="4095" minValue="0"/>
  </inputParameter>

  <inputParameter>
    <name>atwd_analog_ref_dac</name>
    <unsignedInt default="2300" maxValue="4095" minValue="0"/>
  </inputParameter>

  <inputParameter>
    <name>atwd_pedestal_dac</name>
    <unsignedInt default="2200" maxValue="4095" minValue="0"/>
  </inputParameter>

  <inputParameter>
    <name>atwd_chip_a_or_b</name>
    <unsignedInt default="0" maxValue="1" minValue="0"/>
  </inputParameter>

  <inputParameter>
    <name>atwd_channel</name>
    <unsignedInt default="0" maxValue="2" minValue="0"/>
  </inputParameter>

  <inputParameter>
    <name>atwd_trig_forced_or_spe</name>
    <unsignedInt default="0" maxValue="1" minValue="0"/>
  </inputParameter>

  <inputParameter>
    <name>spe_descriminator_uvolt</name>
    <unsignedInt default="0" maxValue="5000" minValue="-5000"/>
  </inputParameter>

  <inputParameter>
    <name>loop_count</name>
    <unsignedInt default="1000" maxValue="1000000" minValue="1"/>
  </inputParameter>

  <inputParameter>
    <name>fill_output_arrays</name>
    <boolean default="false"/>
  </inputParameter>

  <outputParameter>
    <name>atwd_pedestal_amplitude</name>
    <unsignedInt/>
  </outputParameter>

  <outputParameter>
    <name>atwd_pedestal_pattern</name>
    <unsignedInt/>
  </outputParameter>

  <outputParameter>
    <name>atwd_spe_disc_threshold_dac</name>
    <unsignedInt/>
  </outputParameter>


</stf:test>





