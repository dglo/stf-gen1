<?xml version="1.0" encoding="UTF-8"?>
<stf:test xmlns:stf="http://glacier.lbl.gov/icecube/daq/stf">
  <name>ADC</name>
  <description>ADC Tests</description>
  <version major="1" minor="0"/>
  <inputParameter>
    <description>Number of ADC values to acquire to compute a mean and spread</description>
    <name>loop_count</name>
    <unsignedInt default="1000" maxValue="1000000" minValue="1"/>
  </inputParameter>

  <outputParameter>
    <description>Mean of the ADC values distribution in count units</description>
    <name>adc_value</name>
    <unsignedIntArray length="24" />
  </outputParameter>

  <outputParameter>
    <description>Pass or Fail the test</description>
    <name>pass_or_fail</name>
    <unsignedIntArray length="24" />
  </outputParameter>

</stf:test>








