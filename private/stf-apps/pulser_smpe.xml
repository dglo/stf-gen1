<?xml version="1.0" encoding="UTF-8"?>
<stf:test xmlns:stf="http://glacier.lbl.gov/icecube/daq/stf">
  <name>pulser_spe_mpe</name>
  <description>Pulser SPE/MPE discriminator test</description>
  <version major="1" minor="0"/>
  <inputParameter>
    <name>pulse_height</name>
    <unsignedInt default="1000"/>
  </inputParameter>

  <inputParameter>
    <name>discriminator_level</name>
    <unsignedInt default="500"/>
  </inputParameter>

  <inputParameter>
    <name>mpe_discriminator</name>
    <boolean default="False"/>
  </inputParameter>

  <inputParameter>
    <name>pedestal_level</name>
    <unsignedInt default="3000000"/>
  </inputParameter>

  <inputParameter>
    <name>repetition_rate</name>
    <unsignedInt default="0"/>
  </inputParameter>

  <inputParameter>
    <name>max_rate_deviation</name>
    <unsignedInt default="10"/>
  </inputParameter>

  <outputParameter>
    <name>measured_rate</name>
    <unsignedInt/>
  </outputParameter>

</stf:test>





