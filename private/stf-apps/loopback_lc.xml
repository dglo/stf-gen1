<?xml version="1.0" encoding="UTF-8"?>
<stf:test xmlns:stf="http://glacier.lbl.gov/icecube/daq/stf">
  <name>loopback_lc</name>
  <description>Loopback Local Coincidence</description>
  <version major="1" minor="0"/>

  <inputParameter>
    <description>Number of times to run the test</description>
    <name>loop_count</name>
    <unsignedInt default="1000" maxValue="1000000" minValue="1"/>
  </inputParameter>

  <outputParameter>
    <description>Tx quiet</description>
    <name>lc_tx_quiet</name>
    <unsignedInt/>
  </outputParameter>

  <outputParameter>
    <description>Tx down high</description>
    <name>lc_tx_dwn_hi</name>
    <unsignedInt/>
  </outputParameter>

  <outputParameter>
    <description>Tx down low</description>
    <name>lc_tx_dwn_lo</name>
    <unsignedInt/>
  </outputParameter>

  <outputParameter>
    <description>Tx up high</description>
    <name>lc_tx_up_hi</name>
    <unsignedInt/>
  </outputParameter>

  <outputParameter>
    <description>Tx up low</description>
    <name>lc_tx_up_lo</name>
    <unsignedInt/>
  </outputParameter>

  <outputParameter>
    <description>Tx up high down high</description>
    <name>lc_tx_up_hi_dwn_high</name>
    <unsignedInt/>
  </outputParameter>

  <outputParameter>
    <description>Tx up high down low</description>
    <name>lc_tx_up_hi_dwn_lo</name>
    <unsignedInt/>
  </outputParameter>

  <outputParameter>
    <description>Tx up low down low</description>
    <name>lc_tx_up_lo_dwn_lo</name>
    <unsignedInt/>
  </outputParameter>

  <outputParameter>
    <description>Tx up low down high</description>
    <name>lc_tx_up_lo_dwn_hi</name>
    <unsignedInt/>
  </outputParameter>

</stf:test>








