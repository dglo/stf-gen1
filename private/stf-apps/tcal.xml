  <outputParameter>
    <description>DOM waveforms</description>
    <name>dom_waveforms</name>
    <unsignedIntArray length="4800" />
  </outputParameter>
  <outputParameter>
    <description>DOR waveforms</description>
    <name>dor_waveforms</name>
    <unsignedIntArray length="4800" />
  </outputParameter>
