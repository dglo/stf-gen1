#!/bin/sh

#
# include common functions, variables...
#
source /usr/lib/cgi-bin/stf/xml/bin/common.sh

#
# remove old temp files...
#
rm -f /tmp/dhtab.[0-9]+
rm -f /tmp/stf

echo "Content-type: text/html"
echo ""
echo "<html><body>"

${javacmd} alloff domhub2.lbl.gov

echo "<h2>Done</h2>"
echo "</body></html>"
